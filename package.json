{
  "name": "unplugin-overlay-layout",
  "type": "module",
  "version": "1.0.0-beta.2",
  "packageManager": "pnpm@8.6.12",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/motea927/unplugin-overlay-layout",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motea927/unplugin-overlay-layout.git"
  },
  "bugs": {
    "url": "https://github.com/motea927/unplugin-overlay-layout/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:popup": "npm -C popup run build",
    "build:plugin": "tsup",
    "build:fix": "esno scripts/postbuild.ts",
    "build:all": "npm run build:popup && npm run build:plugin && cp -r src/overlay-layout dist/overlay-layout",
    "dev:playground": "npm -C popup run dev & npm -C playground run dev",
    "lint": "eslint .  --config .eslintrc.cjs",
    "release": "npm run build:all && bumpp && npm publish",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    }
  },
  "dependencies": {
    "@vueuse/core": "^10.7.1",
    "sirv": "^2.0.4",
    "unplugin": "^1.5.1"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@nuxt/kit": "^3.8.2",
    "@nuxt/schema": "^3.8.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.3",
    "bumpp": "^9.2.0",
    "chalk": "^5.3.0",
    "eslint": "^8.55.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vitest": "^0.34.6",
    "webpack": "^5.89.0"
  }
}
